    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "busboy-body-parser": "^0.3.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cloudinary": "^1.9.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "nanobar": "^0.4.2",
    "nodemailer": "^4.4.0",
