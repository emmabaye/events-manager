{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "Application for managing event centers. This app will help you accept applications to use your center/facilities, and will either decline events when the proposed day is already taken, or suggest an available day",
  "main": "./server/server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "build-client": "node ./node_modules/webpack/bin/webpack.js -p",
    "build-server": " node ./node_modules/babel-cli/bin/babel.js -d ./build ./server",
    "postinstall": "npm run build-client",
    "heroku-postbuild": "npm run populate-db && npm run build-server",
    "populate-db": "npm run unmigrate && npm run migrate",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate && npm run seed",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "start-dev": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js ./server/server.js",
    "test": "nyc mocha --compilers js:babel-core/register ./server/test --timeout 60000 --exit",
    "test-client": "jest --coverage",
    "test-client-watch": "jest --watch --coverage",
    "e2e-setup": "node node_modules/selenium-standalone/bin/selenium-standalone install",
    "test-e2e": "nightwatch",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "lint": "eslint server",
    "webpack-dev-server": "webpack-dev-server --config webpack.dev.config.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmabaye/events-manager.git"
  },
  "author": "Emmanuel Abaye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emmabaye/events-manager/issues"
  },
  "homepage": "https://github.com/emmabaye/events-manager#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.10.0",
    "babel-jest": "^22.0.4",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.5",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nightwatch": "^0.9.19",
    "node-sass": "^4.7.2",
    "nyc": "^11.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "^6.14.1",
    "sinon": "^6.1.3",
    "style-loader": "^0.19.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
